class ScheduledMessageAdapter extends TypeAdapter<ScheduledMessage> {
  @override
  final int typeId = 3;

  @override
  ScheduledMessage read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return ScheduledMessage(
      id: fields[0] as String,
      phone: fields[1] as String,
      message: fields[2] as String,
      scheduledTime: fields[3] as DateTime,
      isSent: fields[4] as bool,
    );
  }

  @override
  void write(BinaryWriter writer, ScheduledMessage obj) {
    writer
      ..writeByte(5)
      ..writeByte(0)
      ..write(obj.id)
      ..writeByte(1)
      ..write(obj.phone)
      ..writeByte(2)
      ..write(obj.message)
      ..writeByte(3)
      ..write(obj.scheduledTime)
      ..writeByte(4)
      ..write(obj.isSent);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is ScheduledMessageAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
